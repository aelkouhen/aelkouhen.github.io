	[BillingCity] [varchar](20) NULL,
	[BillingAddress] [varchar](50) NULL,
	[InvoiceDate] [varchar](20) NULL,
	[CustomerId] [bigint] NULL,
	[InvoiceId] [bigint] IDENTITY(1,1) NOT NULL,
